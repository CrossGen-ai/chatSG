{
  "name": "backend",
  "version": "1.0.0",
  "description": "ChatSG Backend Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cd tests && node run-tests.js",
    "test:env": "cd tests && node test-env.js",
    "test:llm": "cd tests && node test-llm-helper.js",
    "test:backend": "cd tests && node test-all-backends.js",
    "test:agent": "cd tests && node test-agent-zero.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "generate:agent": "node scripts/generate-agent.js",
    "generate:individual": "node scripts/generate-agent.js individual",
    "generate:agency": "node scripts/generate-agent.js agency",
    "gen": "node scripts/generate-agent.js",
    "dev:setup": "node scripts/dev-agent.js setup",
    "dev:list": "node scripts/dev-agent.js list",
    "dev:analytical": "cd src/agents/individual/analytical && npm run dev:isolated",
    "dev:customer-support": "cd src/agents/agencies/customer-support && npm run dev:isolated",
    "test:analytical": "cd src/agents/individual/analytical && npm run test:isolated",
    "test:customer-support": "cd src/agents/agencies/customer-support && npm run test:isolated"
  },
  "keywords": [
    "chatsg",
    "backend",
    "api",
    "langchain",
    "langgraph"
  ],
  "author": "StartGuides",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.21",
    "@langchain/langgraph": "^0.2.19",
    "@langchain/openai": "^0.3.14",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
